@{
    Layout="Shared/_Layout.cshtml";
}

<h1>Enter how much change you have</h1>

<form action="/coincount" method="post">
    <label for="change">Enter your change (1-100) </label>
    <input type="number" name="change" min="1" max="100">
    <button type="submit" name="button">Submit</button>
</form>
